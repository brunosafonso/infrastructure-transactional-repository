{
	"id": "infrastructure-transactional-repository",
	"container": {
		"portMappings": [
			{
				"containerPort": 5432,
				"hostPort": 25000,
				"labels": {
					"VIP_0": "/infrastructure-transactional-repository:5432"
				}
			}
		],
		"type": "MESOS",
		"volumes": [
			{
				"containerPath": "/var/lib/postgresql/data",
				"hostPath": "postgres_data",
				"mode": "RW"
			},
			{
				"containerPath": "postgres_data",
				"mode": "RW",
				"external": {
					"size": 12,
					"name": "infrastructure-transactional-repository-data",
					"provider": "dvdi",
					"options": {
						"dvdi/driver": "rexray"
					}
				}
			}
		],
		"docker": {
			"image": "coldis/infrastructure-transactional-repository",
			"forcePullImage": true,
			"parameters": []
		}
	},
	"env": {},
	"healthChecks": [],
	"args": [],
	"cpus": 1,
	"disk": 1024,
	"instances": 1,
	"mem": 256
}